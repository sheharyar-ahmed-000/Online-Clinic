@model feedback;
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Bootstrap List Example</title>
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>

    <div class="container">
        <h1>Feedback List</h1>
        <ul class="list-group">
            <li class="list-group-item"><h4>User ID: @Model.feedback_id</h4></li>
            <li class="list-group-item"><h4>User Name: @Model.feedback_name</h4></li>
            <li class="list-group-item"><h4>User Message: @Model.feedback_message</h4></li>
        </ul>
        <div class="mt-3">
            <a href="@Url.Action("addfeedback","admin")"> <button type="button" class="btn btn-success">Add Feedback</button></a>
        </div>
    </div>

    <!-- Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
